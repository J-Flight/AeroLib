(*
GetSymbolColorIn
~~~~~~~~~~~~~~~~

.. code-block:: pascal

    function getSymbolColorIn(var rx, ry: Integer; Name: string; xs, ys, xe, ye: Integer): Integer;

Returns the Color of the Chosen Symbol, Searching in a selected area
of the Minimap

.. note::

  by Starblaster100 based off of the ideas of Stupid3000

Example:

.. code-block:: pascal

*)
function getSymbolColor(var rx, ry: Integer; Name: string): Integer;
var
  TheColor, c, x, y: Integer;
  acc: Extended;
begin
  TheColor := loadSymbolBitmapColor(LowerCase(Name));
  if (TheColor <> 0) then
  begin
    findDeformedBitmapToleranceIn(SymbolBitmap, x, y, MMX1, MMY1, MMX2, MMY2, 50, 0, True, acc);
    if not pointInBox(point(x,y), toBox(568,125,594,152)) then
      if (acc > SymbolAccuracy) then
        repeat
          c := c + 5;
          if findColorTolerance(x, y, TheColor, x, y, x + 15, y + 5, c) then
          begin
            Result := GetColor(x, y);
            rx := x;
            ry := y;
          end;
        until (Result > 0) or (c > 70);
  end else
    warn(Name + ' is not a valid name.', WT_CORE);
  try
    freeBitmap(SymbolBitmap);
  except
    Result := 0;
  finally
    Exit;
  end;
end;

(*
FindSymbol
~~~~~~~~~~

.. code-block:: pascal

    function findSymbol(var rx, ry: Integer; Name: String): Boolean;

Finds a minimap Symbol and returns a Boolean if True.

.. note::

  by Stupid3ooo

Example:

.. code-block:: pascal

*)
function findSymbol(var Pnt: TPoint; Name: string): Boolean;
begin
  Result := (getSymbolColor(Pnt.X, Pnt.Y, LowerCase(Name)) > 0);
end;

(*
FindSymbolsIn
~~~~~~~~~~~~~

.. code-block:: pascal

    function findSymbolsIn(var AnsTPA: TPointArray; SymbolName: string; x1, y1, x2, y2: integer): Boolean;

Finds a symbol in multiple places within the search coords. Results
true if at least 1 symbol is found.

.. note::

  by lordsaturn Fixed by Bad Boy JH

Example:

.. code-block:: pascal

*)
function findSymbols(var AnsTPA: TPointArray; SymbolName: string): Boolean;
var
  Pt           : TPoint;
  tmpCol       : TColEx;
  CTS,Col,Hi,i,
  x,y,fx,fy,L  : Integer;
  acc          : Extended;
  P            : TPointArray;
  aP           : T2DPointArray;
begin
  Col := getSymbolColor(fx, fy, SymbolName);
  if Col = 0 then
    Exit;

  tmpCol.create(Col, 0);
  tmpCol.findAllIn(AREA_MM, P);
  aP := clusterTPA(P, 10);
  Hi := High(aP);
  loadSymbolBitmapColor(LowerCase(SymbolName));

  SetLength(AnsTPA, Hi+1);
  for i := 0 to Hi do
  begin
    Pt := aP[i].midPnt();
    if pointInTPA(Point(fx, fy), aP[i]) then
      acc := 1
    else
      findDeformedBitmapToleranceIn(SymbolBitmap, x, y, Pt.x-15, Pt.y-5, Pt.x+15, Pt.y+5, 70, 0, True, acc);
    if (acc > SymbolAccuracy) then
    begin
      AnsTPA[L] := Pt;
      Inc(L);
    end;
  end;
  Result := L > 0;
  SetLength(AnsTPA, L);
  freeBitmap(SymbolBitmap);
end;

(*
FindSymbolsMultiIn
~~~~~~~~~~~~~~~~~~

.. code-block:: pascal

    function findSymbolsMultiIn(var aTPA: TPointArray; Symbols: TStringArray): Boolean;

Finds multiple symbols the specified area. Results true if at least
1 symbol is found.

.. note::

  by Nava2 & NCDS

Example:

.. code-block:: pascal

*)
function findSymbolsMulti(var aTPA: TPointArray; Symbols: TStringArray): Boolean;
var
  gTPA: TPointArray;
  I: Integer;
begin
  for I:=0 to high(Symbols) do
  begin
    if findSymbols(gTPA, Symbols[i]) then
      Result := True;
    aTPA := combineTPA(gTPA, aTPA);
  end;
end;

function foundSymbol(Name: string): Boolean;
var
  Pnt: TPoint;
begin
  Result := findSymbol(Pnt,Name);
end;
